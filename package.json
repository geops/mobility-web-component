{
  "type": "module",
  "scripts": {
    "build": "yarn build:css && yarn build:js",
    "build:js": "node scripts/build.mjs",
    "build:css": "yarn tailwind:component && yarn tailwind:website",
    "format": "prettier --write 'src/**/*.tsx'",
    "tailwind:component": "tailwindcss --output=src/style.css --content=src/**/*.tsx",
    "tailwind:website": "tailwindcss --output=output.css --content=index.html --minify",
    "dev": "node scripts/dev.mjs",
    "start": "yarn dev",
    "up": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "maplibre-gl": "^3.5.0",
    "mobility-toolbox-js": "^2.2.0-beta.3",
    "ol": "^8.1.0",
    "preact": "^10.18.1",
    "preact-custom-element": "^4.3.0",
    "rosetta": "^1.1.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "esbuild": "^0.19.4",
    "esbuild-sass-plugin": "^2.16.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3"
  }
}
