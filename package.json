{
  "devDependencies": {
    "esbuild": "^0.18.16",
    "esbuild-sass-plugin": "^2.10.0",
    "maplibre-gl": "^3.2.1",
    "mobility-toolbox-js": "^2.0.0-beta.79",
    "ol": "^7.4.0",
    "preact": "^10.16.0",
    "preact-custom-element": "^4.2.1",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build:js": "node scripts/build.mjs",
    "build:css": "npm run tailwind:component && npm run tailwind:website",
    "tailwind:component": "tailwindcss --output=src/style.css --content=src/**/*.tsx",
    "tailwind:website": "tailwindcss --output=output.css --content=index.html --minify",
    "dev": "node scripts/dev.mjs"
  },
  "type": "module"
}
